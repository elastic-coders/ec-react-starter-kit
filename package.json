{
  "name": "ec-react-starter-kit",
  "private": true,
  "version": "0.1.0",
  "ecTools": {
    "clean": [
      "./dist/*"
    ],
    "copy": {
      "./src/static": "./dist/static"
    },
    "copyPackage": {
      "scripts": {
        "start": "node ./server"
      }
    }
  },
  "main": "dist/server.js",
  "scripts": {
    "start": "ec-tools serve devProxy watch",
    "lint": "eslint .",
    "test": "jest --coverage '/src/'",
    "devtool": "npm run build && devtool -r ./dist/server.js",
    "clean": "ec-tools clean",
    "copy": "ec-tools copy",
    "bundle": "ec-tools bundle",
    "build": "ec-tools build",
    "image": "ec-tools image"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.s?css$": "<rootDir>/src/__mocks__/styles.mock.js"
    }
  },
  "author": "Nicola Peduzzi <thenikso@gmail.com> (http://nikso.net)",
  "license": "MIT",
  "devDependencies": {
    "JSONStream": "1.1.4",
    "autoprefixer": "6.4.0",
    "babel-cli": "6.14.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-jest": "15.0.0",
    "babel-loader": "^6.0.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babel-root-import": "4.1.3",
    "css-loader": "0.24.0",
    "cssnano": "3.7.4",
    "dockerode": "2.3.0",
    "ec-node-tools": "0.2.2",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "fs-extra-promise": "0.4.0",
    "gaze": "1.1.1",
    "git-describe": "3.0.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "15.1.1",
    "json-loader": "0.5.4",
    "postcss": "5.1.2",
    "postcss-loader": "0.11.0",
    "precss": "1.4.0",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "15.3.1",
    "redux-logger": "2.6.1",
    "style-loader": "0.13.1",
    "tar-fs": "1.13.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "auth0-lock": "10.2.1",
    "babel-polyfill": "6.13.0",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "express": "4.14.0",
    "jwt-decode": "2.1.0",
    "morgan": "1.7.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "redux": "3.6.0",
    "redux-actions": "0.11.0",
    "redux-promise": "0.5.3"
  }
}
